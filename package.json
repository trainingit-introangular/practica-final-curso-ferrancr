    "@angular/router": "~9.1.1",
    "core-js": "^3.6.5",
    "mini.css": "^3.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
